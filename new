
(cd /home/zeal/buildroot-2009.05/toolchain_build_arm/binutils-2.19.1-build; rm -rf config.cache; \
		PATH=xx
		AR="/usr/bin/ar"
		AS="/usr/bin/as"
		LD="/usr/bin/ld
		-L/home/zeal/buildroot-2009.05/build_arm/host_dir/lib
		-L/home/zeal/buildroot-2009.05/build_arm/host_dir/usr/lib"
		NM="/usr/bin/nm"
		CC="/usr/bin/gcc
		-I/home/zeal/buildroot-2009.05/build_arm/host_dir/include
		-I/home/zeal/buildroot-2009.05/build_arm/host_dir/usr/include"
		GCC="/usr/bin/gcc
		-I/home/zeal/buildroot-2009.05/build_arm/host_dir/include
		-I/home/zeal/buildroot-2009.05/build_arm/host_dir/usr/include"
		CXX="/usr/bin/g++
		-I/home/zeal/buildroot-2009.05/build_arm/host_dir/include
		-I/home/zeal/buildroot-2009.05/build_arm/host_dir/usr/include"
		CPP="/usr/bin/cpp
		-I/home/zeal/buildroot-2009.05/build_arm/host_dir/include
		-I/home/zeal/buildroot-2009.05/build_arm/host_dir/usr/include"
		AR_FOR_BUILD="/usr/bin/ar"
		AS_FOR_BUILD="/usr/bin/as"
		CC_FOR_BUILD="/usr/bin/gcc"
		GCC_FOR_BUILD="/usr/bin/gcc"
		CXX_FOR_BUILD="/usr/bin/g++"
		LD_FOR_BUILD="/usr/bin/ld"
		FC_FOR_BUILD="/usr/bin/ld"
		CFLAGS_FOR_BUILD="-I/home/zeal/buildroot-2009.05/build_arm/host_dir/include
		-I/home/zeal/buildroot-2009.05/build_arm/host_dir/usr/include"
		CXXFLAGS_FOR_BUILD="-I/home/zeal/buildroot-2009.05/build_arm/host_dir/include
		-I/home/zeal/buildroot-2009.05/build_arm/host_dir/usr/include"
		LDFLAGS_FOR_BUILD="-L/home/zeal/buildroot-2009.05/build_arm/host_dir/lib
		-L/home/zeal/buildroot-2009.05/build_arm/host_dir/usr/lib"
		FCFLAGS_FOR_BUILD=""
		AR_FOR_TARGET="/staging_dir/usr/bin/arm-linux-uclibc-ar"
		AS_FOR_TARGET="/staging_dir/usr/bin/arm-linux-uclibc-as"
		CC_FOR_TARGET="/staging_dir/usr/bin/arm-linux-uclibc-gcc"
		LD_FOR_TARGET="/staging_dir/usr/bin/arm-linux-uclibc-ld"
		NM_FOR_TARGET="/staging_dir/usr/bin/arm-linux-uclibc-nm"
		RANLIB_FOR_TARGET="/staging_dir/usr/bin/arm-linux-uclibc-ranlib"
		STRIP_FOR_TARGET="/staging_dir/usr/bin/arm-linux-uclibc-strip"
		OBJCOPY_FOR_TARGET="/staging_dir/usr/bin/arm-linux-uclibc-objcopy"
		OBJDUMP_FOR_TARGET="/staging_dir/usr/bin/arm-linux-uclibc-objdump"
		DEFAULT_ASSEMBLER="/staging_dir/usr/bin/arm-linux-uclibc-as"
		DEFAULT_LINKER="/staging_dir/usr/bin/arm-linux-uclibc-ld"
		ORIGINAL_AS_FOR_TARGET="/staging_dir/usr/bin/arm-linux-uclibc-as"
		ORIGINAL_NM_FOR_TARGET="/staging_dir/usr/bin/arm-linux-uclibc-nm"
		ORIGINAL_OBJDUMP_FOR_TARGET="/staging_dir/usr/bin/arm-linux-uclibc-objdump"
		PKG_CONFIG="build_arm/host_dir/usr/bin/pkg-config"
		PKG_CONFIG_PATH="build_arm/host_dir/usr/lib/pkgconfig"
		\
		/home/zeal/buildroot-2009.05/toolchain_build_arm/binutils-2.19.1/configure \

		--prefix=/home/zeal/buildroot-2009.05/build_arm/staging_dir/usr \
		--build=i386-pc-linux-gnu \
		--host=i386-pc-linux-gnu \
		--target=arm-linux-uclibc \
		--mandir=/home/zeal/buildroot-2009.05/build_arm/staging_dir/usr/share/man \
		--infodir=/home/zeal/buildroot-2009.05/build_arm/staging_dir/usr/share/info \
		--with-sysroot=/home/zeal/buildroot-2009.05/toolchain_build_arm/uClibc_dev/ \
		 \
		--disable-nls \
		--disable-multilib \
		--disable-werror \
		 \
		 --with-gmp="/home/zeal/buildroot-2009.05/toolchain_build_arm/gmp" --with-mpfr="/home/zeal/buildroot-2009.05/toolchain_build_arm/mpfr" \
		 \
	)
